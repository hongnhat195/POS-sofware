    "test": "jest"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mssql": "^7.2.1",
    "nodemon": "^2.0.15",
    "sequelize": "^6.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "sequelize-cli": "^6.3.0"
  }
}
